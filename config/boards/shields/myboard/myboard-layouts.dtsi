/* myboard-layouts.dtsi */

/ {
    key_physical_attrs: key_physical_attrs {
        compatible = "zmk,key-physical-attrs";
        #binding-cells = <0>;
        #key-cells = <7>;
    };

    physical_layout0: physical_layout_0 {
        compatible = "zmk,physical-layout";
        display-name = "Default Layout";
        transform = <&default_transform>;
        kscan = <&kscan0>;
        
        /* * 1u key size = 1900 units (19mm) 
         * Format: <&key_physical_attrs w h x y rot rx ry>
         */
        keys  // Row 1
            = <&key_physical_attrs 1900 1900 0 0 0 0 0>
            , <&key_physical_attrs 1900 1900 1900 0 0 0 0>
            , <&key_physical_attrs 1900 1900 3800 0 0 0 0>
            , <&key_physical_attrs 1900 1900 5700 0 0 0 0>
            , <&key_physical_attrs 1900 1900 7600 0 0 0 0>
            , <&key_physical_attrs 1900 1900 9500 0 0 0 0>
            , <&key_physical_attrs 1900 1900 11400 0 0 0 0>
            , <&key_physical_attrs 1900 1900 13300 0 0 0 0>
            , <&key_physical_attrs 1900 1900 15200 0 0 0 0>
            , <&key_physical_attrs 1900 1900 17100 0 0 0 0>
            , <&key_physical_attrs 1900 1900 19000 0 0 0 0>
            , <&key_physical_attrs 1900 1900 20900 0 0 0 0>

            // Row 2
            , <&key_physical_attrs 1900 1900 0 1900 0 0 0>
            , <&key_physical_attrs 1900 1900 1900 1900 0 0 0>
            , <&key_physical_attrs 1900 1900 3800 1900 0 0 0>
            , <&key_physical_attrs 1900 1900 5700 1900 0 0 0>
            , <&key_physical_attrs 1900 1900 7600 1900 0 0 0>
            , <&key_physical_attrs 1900 1900 9500 1900 0 0 0>
            , <&key_physical_attrs 1900 1900 11400 1900 0 0 0>
            , <&key_physical_attrs 1900 1900 13300 1900 0 0 0>
            , <&key_physical_attrs 1900 1900 15200 1900 0 0 0>
            , <&key_physical_attrs 1900 1900 17100 1900 0 0 0>
            , <&key_physical_attrs 1900 1900 19000 1900 0 0 0>
            , <&key_physical_attrs 1900 1900 20900 1900 0 0 0>

            // Row 3
            , <&key_physical_attrs 1900 1900 0 3800 0 0 0>
            , <&key_physical_attrs 1900 1900 1900 3800 0 0 0>
            , <&key_physical_attrs 1900 1900 3800 3800 0 0 0>
            , <&key_physical_attrs 1900 1900 5700 3800 0 0 0>
            , <&key_physical_attrs 1900 1900 7600 3800 0 0 0>
            , <&key_physical_attrs 1900 1900 9500 3800 0 0 0>
            , <&key_physical_attrs 1900 1900 11400 3800 0 0 0>
            , <&key_physical_attrs 1900 1900 13300 3800 0 0 0>
            , <&key_physical_attrs 1900 1900 15200 3800 0 0 0>
            , <&key_physical_attrs 1900 1900 17100 3800 0 0 0>
            , <&key_physical_attrs 1900 1900 19000 3800 0 0 0>
            , <&key_physical_attrs 1900 1900 20900 3800 0 0 0>

            // Row 4
            , <&key_physical_attrs 1900 1900 0 5700 0 0 0>
            , <&key_physical_attrs 1900 1900 1900 5700 0 0 0>
            , <&key_physical_attrs 1900 1900 3800 5700 0 0 0>
            , <&key_physical_attrs 1900 1900 5700 5700 0 0 0>
            , <&key_physical_attrs 1900 1900 7600 5700 0 0 0>
            , <&key_physical_attrs 1900 1900 9500 5700 0 0 0>
            , <&key_physical_attrs 1900 1900 11400 5700 0 0 0>
            , <&key_physical_attrs 1900 1900 13300 5700 0 0 0>
            , <&key_physical_attrs 1900 1900 15200 5700 0 0 0>
            , <&key_physical_attrs 1900 1900 17100 5700 0 0 0>
            , <&key_physical_attrs 1900 1900 19000 5700 0 0 0>
            , <&key_physical_attrs 1900 1900 20900 5700 0 0 0>
            ;
    };
};
